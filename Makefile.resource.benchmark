# === resource benchmark simulation === 

resource_bench_1_3_$(DFEModel)_DFE_SIM/results/resource_bench_1_3.max: $(ENGINEFILES)
	$(MAXJC) $(JFLAGS) $(ENGINEFILES)
	MAXAPPJCP='.:$(CP)' MAXSOURCEDIRS='../src' $(MAXJAVARUN) -v -m 8192 $(MANAGER) DFEModel=$(DFEModel) maxFileName=resource_bench_1_3 target='DFE_SIM' enableMPCX=$(MPCX) convFoldingFactor=1 kernelFoldingFactor=3

resource_bench_2_4_$(DFEModel)_DFE_SIM/results/resource_bench_2_4.max: $(ENGINEFILES)
